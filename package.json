    "@types/eslint": "^8.4.6",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/jest": "^29.0.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^18.7.18",
    "@types/prettier": "^2.7.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-html-parser": "^2.0.2",
    "@types/reflect-metadata": "^0.1.0",
