    "eslint-plugin-prettier": "^5.0.0-alpha.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^11.1.4",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
